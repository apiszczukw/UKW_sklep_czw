@model IEnumerable<Category>

<div class="collapse navbar-collapse" id="myNavbar">
    <ul class="nav navbar-nav">
        @foreach (var category in Model)
        {
            <li><a href="@Url.Action("FilmsList", "Films", new { categoryName = category.Name })">@category.Name</a></li>
        }
            <li><a class="btn btn-danger" href="@Url.Action("AllFilms", "Films")">Wszystkie</a></li>
    </ul>
    
</div>